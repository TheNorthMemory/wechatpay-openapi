{
    "name": "wechatpay/openapi",
    "description": "这是一个纯接口描述包，源文件由程序自动生成，旨在为IDE提供结构提示，无他用。",
    "keywords": [
        "wechatpay",
        "openapi-chainable",
        "interfaces"
    ],
    "authors": [
        {
            "name": "James ZHANG",
            "homepage": "https://github.com/TheNorthMemory"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "psr/http-message": "^1.0",
        "guzzlehttp/promises": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "WeChatPay\\OpenAPI\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.89"
    }
}
