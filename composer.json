{
    "name": "iwechatpay/openapi",
    "description": "为 wechatpay/wechatpay 增加IDE提示的接口描述包。",
    "keywords": [
        "wechatpay",
        "openapi-chainable",
        "interfaces"
    ],
    "authors": [
        {
            "name": "James ZHANG",
            "homepage": "https://github.com/TheNorthMemory"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.2",
        "psr/http-message": "^1.0 || ^2.0",
        "guzzlehttp/promises": "^1.0 || ^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WeChatPay\\OpenAPI\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.89 || ^1.0 || 1.11.x-dev"
    }
}
